{
  "name": "reports-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch --env-file .env",
    "build": "nest build",
    "start": "nest start --env-file .env",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cross-env DATABASE_URL=:memory: vitest run --config ./vitest.config.e2e.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "db:push": "dotenv -e .env -- drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:reset": "dotenv -e .env -- node reset-db.js && pnpm run db:migrate && pnpm run db:seed",
    "db:seed": "dotenv -e .env -- tsx src/database/seeds/seed-all.ts",
    "parse:war-rooms": "node scripts/parse-war-rooms.js",
    "parse:sessions-orders": "node scripts/parse-sessions-orders.js",
    "parse:monthly-report": "node scripts/parse-monthly-report.js",
    "parse:weekly-corrective": "node scripts/parse-weekly-corrective.js"
  },
  "dependencies": {
    "@libsql/client": "0.15.15",
    "@nestjs/common": "11.0.0",
    "@nestjs/core": "11.0.0",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/swagger": "11.2.1",
    "@repo/database": "workspace:*",
    "@repo/reports": "workspace:*",
    "drizzle-orm": "0.44.7",
    "luxon": "3.7.2",
    "nestjs-zod": "5.0.1",
    "reflect-metadata": "0.2.0",
    "rxjs": "7.8.1",
    "xlsx": "0.18.5",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "10.1.0",
    "@repo/vitest-config": "workspace:*",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@swc/core": "1.10.1",
    "@types/express": "4.17.17",
    "@types/luxon": "3.7.1",
    "@types/node": "24.9.2",
    "@types/supertest": "6.0.0",
    "@vitest/ui": "4.0.8",
    "cross-env": "10.1.0",
    "dotenv-cli": "11.0.0",
    "drizzle-kit": "0.31.6",
    "drizzle-seed": "0.3.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.20.6",
    "typescript": "5.8.2",
    "unplugin-swc": "1.5.8",
    "vitest": "4.0.8",
    "vitest-mock-extended": "3.1.0"
  }
}
